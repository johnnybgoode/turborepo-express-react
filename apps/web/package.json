{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rm -rf dist",
    "dev": "pnpm dotenv-run vite --clearScreen false",
    "dotenv-run": "pnpx @dotenvx/dotenvx run -- ",
    "lint": "biome check --diagnostic-level=warn --write",
    "preview": "vite preview",
    "test": "vitest run",
    "postinstall": "[ ! -e .env ] && cp -v .env.example .env || :"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@repo/biome-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@repo/vitest-config": "workspace:*",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "happy-dom": "^20.5.0",
    "msw": "^2.12.9",
    "playwright": "^1.58.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vitest-browser-react": "^2.0.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
